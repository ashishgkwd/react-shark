{   
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-plusplus": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "jsx-a11y/interactive-supports-focus": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "indent": [0, 4]
    },
    "plugins": [
        "react"
    ]
}