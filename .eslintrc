{   
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "react/jsx-indent": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "indent": ["warn", 4]
    },
    "plugins": [
        "react"
    ]
}